select *
from (select date_format(timestamp '2023-06-08 23:24:25.123', 'year')    as year_c,
             date_format(timestamp '2023-06-08 23:24:25.123', 'month')   as month_c,
             date_format(timestamp '2023-06-08 23:24:25.123', 'day')     as day_c,
             date_format(timestamp '2023-06-08 23:24:25.123', 'hour')    as hour_c,
             date_format(timestamp '2023-06-08 23:24:25.123', 'minute')  as minute_c,
             date_format(timestamp '2023-06-08 23:24:25.123', 'minutes') as minutes_c,
             date_format(timestamp '2023-06-08 23:24:25.123', 'second')  as second_c,
             date_format(timestamp '2023-06-08 23:24:25.123', 'seconds') as seconds_c,
             timestamp '2023-06-08 23:24:25.123'                         as val
      from TEST_KYLIN_FACT limit 1) t1
union all
select *
from (select date_format(timestamp '0023-11-22 03:04:05.123', 'year')    as year_c,
             date_format(timestamp '0023-11-22 03:04:05.123', 'month')   as month_c,
             date_format(timestamp '0023-11-22 03:04:05.123', 'day')     as day_c,
             date_format(timestamp '0023-11-22 03:04:05.123', 'hour')    as hour_c,
             date_format(timestamp '0023-11-22 03:04:05.123', 'minute')  as minute_c,
             date_format(timestamp '0023-11-22 03:04:05.123', 'minutes') as minutes_c,
             date_format(timestamp '0023-11-22 03:04:05.123', 'second')  as second_c,
             date_format(timestamp '0023-11-22 03:04:05.123', 'seconds') as seconds_c,
             timestamp '0023-11-22 03:04:05.123'                         as val
      from TEST_KYLIN_FACT limit 1) t2
